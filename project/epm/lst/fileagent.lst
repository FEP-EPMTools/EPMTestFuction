Nstatic FRESULT fatfsMkdir(char* dir)
N{
N    //reval = f_mkdir("0:\\Test1");
N    //reval = f_mkdir("0:\\Test1\\Test2");
N    //reval = f_mkdir("0:\\Test1\\Test2\\Test3");
N    int dirLength = strlen(dir);
N    if (dir[dirLength-1] == ':') {
N        return FR_OK;
N    }
N    //If last character is slash symbol, cut it !!
N    if ((dir[dirLength-1] == '\\') || (dir[dirLength-1] == '/'))
N    {
N        dir[dirLength-1] = 0x00;
N        dirLength--;
N    }
N    
N    int pathLevel = 0;
N    int i;
N    //Calculate the number of Levels of sub-directory
N    for (i = 0 ; i < dirLength ; i++)
N    {
N        if ((dir[i] == '\\') || (dir[i] == '/')) {
N            pathLevel++;
N        }
N    }
N    if (pathLevel == 0) {
N        return FR_OK;
N    }
N    
N    char pathName[256];
N    int buffIdx = 0;
N    memset(pathName, 0x00, sizeof(pathName));
N    //Check and mkdir ==> level by level
N    for (i = 1 ; i <= pathLevel ; i++)
N    {
N        int hits = 0;
N        for (buffIdx = 0 ; buffIdx < dirLength ; buffIdx++)
N        {
N            if ((dir[buffIdx] == '\\') || (dir[buffIdx] == '/'))
N            {
N                hits++;
N                if (hits == (i + 1)) {
N                    break;
N                }
N            }
N            pathName[buffIdx] = dir[buffIdx];
N        }
N        if (f_chdir(pathName) != FR_OK)
N        {
N            FRESULT reval = f_mkdir(pathName);
N            if (reval != FR_OK) {
N                return reval;
N            }
N        }
N    }
N    return FR_OK;
N}
