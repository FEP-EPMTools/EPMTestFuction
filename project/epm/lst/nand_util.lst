Sstatic size_t drop_ffs(const nand_info_t *nand, const u_char *buf,
S			const size_t *len)
S{
S	size_t i, l = *len;
S
S	for (i = l - 1; i >= 0; i--)
S		if (buf[i] != 0xFF)
S			break;
S
S	/* The resulting length must be aligned to the minimum flash I/O size */
S	l = i + 1;
S	l = (l + nand->writesize - 1) / nand->writesize;
S	l *=  nand->writesize;
S
S	/*
S	 * since the input length may be unaligned, prevent access past the end
S	 * of the buffer
S	 */
S	return min(l, *len);
S}
