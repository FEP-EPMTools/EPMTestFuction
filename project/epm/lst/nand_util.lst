N			percent = (int)n;
N
N			/* output progress message only at whole percent
N			 * steps to reduce the number of messages printed
N			 * on (slow) serial consoles
N			 */
N			if (percent != percent_complete) {
N				percent_complete = percent;
N
N				sysprintf("\rErasing at 0x%llx -- %3d%% complete.",
N				       erase.addr, percent);
N
N				if (opts->jffs2 && result == 0)
N					sysprintf(" Cleanmarker written at 0x%llx.",
N					       erase.addr);
N			}
N		}
N	}
N	if (!opts->quiet)
N		sysprintf("\n");
N
N	if (opts->scrub)
N		chip->scan_bbt(meminfo);
N
N	return 0;
N}
