S
S/**
S * check_pattern:
S *
S * Check if buffer contains only a certain byte pattern.
S *
S * @param buf buffer to check
S * @param patt the pattern to check
S * @param size buffer size in bytes
S * @return 1 if there are only patt bytes in buf
S *         0 if something else was found
S */
Sstatic int check_pattern(const u_char *buf, u_char patt, int size)
S{
S	int i;
S
S	for (i = 0; i < size; i++)
S		if (buf[i] != patt)
S			return 0;
S	return 1;
S}
S
S/**
S * nand_torture:
S *
S * Torture a block of NAND flash.
S * This is useful to determine if a block that caused a write error is still
S * good or should be marked as bad.
S *
S * @param nand NAND device
S * @param offset offset in flash
S * @return 0 if the block is still good
S */
Sint nand_torture(nand_info_t *nand, loff_t offset)
S{
S	u_char patterns[] = {0xa5, 0x5a, 0x00};
S	struct erase_info instr = {
S		.mtd = nand,
S		.addr = offset,
S		.len = nand->erasesize,
S	};
S	size_t retlen;
S	int err, ret = -1, i, patt_count;
S	u_char *buf;
S
S	if ((offset & (nand->erasesize - 1)) != 0) {
S		puts("Attempt to torture a block at a non block-aligned offset\n");
S		return -EINVAL;
S	}
S
S	if (offset + nand->erasesize > nand->size) {
S		puts("Attempt to torture a block outside the flash area\n");
S		return -EINVAL;
S	}
S
S	patt_count = ARRAY_SIZE(patterns);
S
S	buf = yaffs_malloc(nand->erasesize);
S	if (buf == NULL) {
S		puts("Out of memory for erase block buffer\n");
S		return -ENOMEM;
S	}
S
S	for (i = 0; i < patt_count; i++) {
S		err = nand->erase(nand, &instr);
S		if (err) {
S			sysprintf("%s: erase() failed for block at 0x%llx: %d\n",
S				nand->name, instr.addr, err);
S			goto out;
S		}
S
S		/* Make sure the block contains only 0xff bytes */
S		err = nand->read(nand, offset, nand->erasesize, &retlen, buf);
S		if ((err && err != -EUCLEAN) || retlen != nand->erasesize) {
S			sysprintf("%s: read() failed for block at 0x%llx: %d\n",
S				nand->name, instr.addr, err);
S			goto out;
S		}
S
S		err = check_pattern(buf, 0xff, nand->erasesize);
S		if (!err) {
S			sysprintf("Erased block at 0x%llx, but a non-0xff byte was found\n",
S				offset);
S			ret = -EIO;
S			goto out;
S		}
S
S		/* Write a pattern and check it */
S		memset(buf, patterns[i], nand->erasesize);
S		err = nand->write(nand, offset, nand->erasesize, &retlen, buf);
S		if (err || retlen != nand->erasesize) {
S			sysprintf("%s: write() failed for block at 0x%llx: %d\n",
S				nand->name, instr.addr, err);
S			goto out;
S		}
S
S		err = nand->read(nand, offset, nand->erasesize, &retlen, buf);
S		if ((err && err != -EUCLEAN) || retlen != nand->erasesize) {
S			sysprintf("%s: read() failed for block at 0x%llx: %d\n",
S				nand->name, instr.addr, err);
S			goto out;
S		}
S
S		err = check_pattern(buf, patterns[i], nand->erasesize);
S		if (!err) {
S			sysprintf("Pattern 0x%.2x checking failed for block at "
S					"0x%llx\n", patterns[i], offset);
S			ret = -EIO;
S			goto out;
S		}
S	}
S
S	ret = 0;
S
Sout:
S	yaffs_free(buf);
S	return ret;
S}
