N/**
N * check_skip_len
N *
N * Check if there are any bad blocks, and whether length including bad
N * blocks fits into device
N *
N * @param nand NAND device
N * @param offset offset in flash
N * @param length image length
N * @return 0 if the image fits and there are no bad blocks
N *         1 if the image fits, but there are bad blocks
N *        -1 if the image does not fit
N */
Nstatic int check_skip_len(nand_info_t *nand, loff_t offset, size_t length)
N{
N	size_t len_excl_bad = 0;
N	int ret = 0;
N
N	while (len_excl_bad < length) {
N		size_t block_len, block_off;
N		loff_t block_start;
N
N		if (offset >= nand->size)
N			return -1;
N
N		block_start = offset & ~(loff_t)(nand->erasesize - 1);
N		block_off = offset & (nand->erasesize - 1);
N		block_len = nand->erasesize - block_off;
N
N		if (!nand_block_isbad(nand, block_start))
N			len_excl_bad += block_len;
N		else
N			ret = 1;
N
N		offset += block_len;
N	}
N
N	return ret;
N}
