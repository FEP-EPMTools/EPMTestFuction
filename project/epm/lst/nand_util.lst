S
S/******************************************************************************
S * Support for locking / unlocking operations of some NAND devices
S *****************************************************************************/
S
S/**
S * nand_lock: Set all pages of NAND flash chip to the LOCK or LOCK-TIGHT
S *	      state
S *
S * @param mtd		nand mtd instance
S * @param tight		bring device in lock tight mode
S *
S * @return		0 on success, -1 in case of error
S *
S * The lock / lock-tight command only applies to the whole chip. To get some
S * parts of the chip lock and others unlocked use the following sequence:
S *
S * - Lock all pages of the chip using nand_lock(mtd, 0) (or the lockpre pin)
S * - Call nand_unlock() once for each consecutive area to be unlocked
S * - If desired: Bring the chip to the lock-tight state using nand_lock(mtd, 1)
S *
S *   If the device is in lock-tight state software can't change the
S *   current active lock/unlock state of all pages. nand_lock() / nand_unlock()
S *   calls will fail. It is only posible to leave lock-tight state by
S *   an hardware signal (low pulse on _WP pin) or by power down.
S */
Sint nand_lock(struct mtd_info *mtd, int tight)
S{
S	int ret = 0;
S	int status;
S	struct nand_chip *chip = mtd->priv;
S
S	/* select the NAND device */
S	chip->select_chip(mtd, 0);
S
S	/* check the Lock Tight Status */
S	chip->cmdfunc(mtd, NAND_CMD_LOCK_STATUS, -1, 0);
S	if (chip->read_byte(mtd) & NAND_LOCK_STATUS_TIGHT) {
S		sysprintf("nand_lock: Device is locked tight!\n");
S		ret = -1;
S		goto out;
S	}
S
S	chip->cmdfunc(mtd,
S		      (tight ? NAND_CMD_LOCK_TIGHT : NAND_CMD_LOCK),
S		      -1, -1);
S
S	/* call wait ready function */
S	status = chip->waitfunc(mtd, chip);
S
S	/* see if device thinks it succeeded */
S	if (status & 0x01) {
S		ret = -1;
S	}
S
S out:
S	/* de-select the NAND device */
S	chip->select_chip(mtd, -1);
S	return ret;
S}
S
S/**
S * nand_get_lock_status: - query current lock state from one page of NAND
S *			   flash
S *
S * @param mtd		nand mtd instance
S * @param offset	page address to query (must be page-aligned!)
S *
S * @return		-1 in case of error
S *			>0 lock status:
S *			  bitfield with the following combinations:
S *			  NAND_LOCK_STATUS_TIGHT: page in tight state
S *			  NAND_LOCK_STATUS_UNLOCK: page unlocked
S *
S */
Sint nand_get_lock_status(struct mtd_info *mtd, loff_t offset)
S{
S	int ret = 0;
S	int chipnr;
S	int page;
S	struct nand_chip *chip = mtd->priv;
S
S	/* select the NAND device */
S	chipnr = (int)(offset >> chip->chip_shift);
S	chip->select_chip(mtd, chipnr);
S
S
S	if ((offset & (mtd->writesize - 1)) != 0) {
S		sysprintf("nand_get_lock_status: "
S			"Start address must be beginning of "
S			"nand page!\n");
S		ret = -1;
S		goto out;
S	}
S
S	/* check the Lock Status */
S	page = (int)(offset >> chip->page_shift);
S	chip->cmdfunc(mtd, NAND_CMD_LOCK_STATUS, -1, page & chip->pagemask);
S
S	ret = chip->read_byte(mtd) & (NAND_LOCK_STATUS_TIGHT
S					  | NAND_LOCK_STATUS_UNLOCK);
S
S out:
S	/* de-select the NAND device */
S	chip->select_chip(mtd, -1);
S	return ret;
S}
S
S/**
S * nand_unlock: - Unlock area of NAND pages
S *		  only one consecutive area can be unlocked at one time!
S *
S * @param mtd		nand mtd instance
S * @param start		start byte address
S * @param length	number of bytes to unlock (must be a multiple of
S *			page size nand->writesize)
S * @param allexcept	if set, unlock everything not selected
S *
S * @return		0 on success, -1 in case of error
S */
Sint nand_unlock(struct mtd_info *mtd, loff_t start, size_t length,
S	int allexcept)
S{
S	int ret = 0;
S	int chipnr;
S	int status;
S	int page;
S	struct nand_chip *chip = mtd->priv;
S
S	debug("nand_unlock%s: start: %08llx, length: %d!\n",
S		allexcept ? " (allexcept)" : "", start, length);
S
S	/* select the NAND device */
S	chipnr = (int)(start >> chip->chip_shift);
S	chip->select_chip(mtd, chipnr);
S
S	/* check the WP bit */
S	chip->cmdfunc(mtd, NAND_CMD_STATUS, -1, -1);
S	if (!(chip->read_byte(mtd) & NAND_STATUS_WP)) {
S		sysprintf("nand_unlock: Device is write protected!\n");
S		ret = -1;
S		goto out;
S	}
S
S	/* check the Lock Tight Status */
S	page = (int)(start >> chip->page_shift);
S	chip->cmdfunc(mtd, NAND_CMD_LOCK_STATUS, -1, page & chip->pagemask);
S	if (chip->read_byte(mtd) & NAND_LOCK_STATUS_TIGHT) {
S		sysprintf("nand_unlock: Device is locked tight!\n");
S		ret = -1;
S		goto out;
S	}
S
S	if ((start & (mtd->erasesize - 1)) != 0) {
S		sysprintf("nand_unlock: Start address must be beginning of "
S			"nand block!\n");
S		ret = -1;
S		goto out;
S	}
S
S	if (length == 0 || (length & (mtd->erasesize - 1)) != 0) {
S		sysprintf("nand_unlock: Length must be a multiple of nand block "
S			"size %08x!\n", mtd->erasesize);
S		ret = -1;
S		goto out;
S	}
S
S	/*
S	 * Set length so that the last address is set to the
S	 * starting address of the last block
S	 */
S	length -= mtd->erasesize;
S
S	/* submit address of first page to unlock */
S	chip->cmdfunc(mtd, NAND_CMD_UNLOCK1, -1, page & chip->pagemask);
S
S	/* submit ADDRESS of LAST page to unlock */
S	page += (int)(length >> chip->page_shift);
S
S	/*
S	 * Page addresses for unlocking are supposed to be block-aligned.
S	 * At least some NAND chips use the low bit to indicate that the
S	 * page range should be inverted.
S	 */
S	if (allexcept)
S		page |= 1;
S
S	chip->cmdfunc(mtd, NAND_CMD_UNLOCK2, -1, page & chip->pagemask);
S
S	/* call wait ready function */
S	status = chip->waitfunc(mtd, chip);
S	/* see if device thinks it succeeded */
S	if (status & 0x01) {
S		/* there was an error */
S		ret = -1;
S		goto out;
S	}
S
S out:
S	/* de-select the NAND device */
S	chip->select_chip(mtd, -1);
S	return ret;
S}
