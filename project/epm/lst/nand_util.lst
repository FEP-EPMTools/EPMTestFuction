N/**
N * nand_read_skip_bad:
N *
N * Read image from NAND flash.
N * Blocks that are marked bad are skipped and the next block is read
N * instead as long as the image is short enough to fit even after skipping the
N * bad blocks.
N *
N * @param nand NAND device
N * @param offset offset in flash
N * @param length buffer length, on return holds number of read bytes
N * @param buffer buffer to write to
N * @return 0 in case of success
N */
Nint nand_read_skip_bad(nand_info_t *nand, loff_t offset, size_t *length,
N		       u_char *buffer)
N{
N	int rval;
N	size_t left_to_read = *length;
N	u_char *p_buffer = buffer;
N	int need_skip;
N
N	if ((offset & (nand->writesize - 1)) != 0) {
N		sysprintf("Attempt to read non page-aligned data\n");
N		*length = 0;
N		return -EINVAL;
X		return -22;
N	}
N
N	need_skip = check_skip_len(nand, offset, *length);
N	if (need_skip < 0) {
N		sysprintf("Attempt to read outside the flash area\n");
N		*length = 0;
N		return -EINVAL;
X		return -22;
N	}
N
N	if (!need_skip) {
N		rval = nand_read(nand, offset, length, buffer);
N		if (!rval || rval == -EUCLEAN)
X		if (!rval || rval == -117)
N			return 0;
N
N		*length = 0;
N		sysprintf("NAND read from offset %llx failed %d\n",
N			offset, rval);
N		return rval;
N	}
N
N	while (left_to_read > 0) {
N		size_t block_offset = offset & (nand->erasesize - 1);
N		size_t read_length;
N
N		if (nand_block_isbad(nand, offset & ~(nand->erasesize - 1))) {
N			sysprintf("Skipping bad block 0x%08llx\n",
N				offset & ~(nand->erasesize - 1));
N			offset += nand->erasesize - block_offset;
N			continue;
N		}
N
N		if (left_to_read < (nand->erasesize - block_offset))
N			read_length = left_to_read;
N		else
N			read_length = nand->erasesize - block_offset;
N
N		rval = nand_read(nand, offset, &read_length, p_buffer);
N		if (rval && rval != -EUCLEAN) {
X		if (rval && rval != -117) {
N			sysprintf("NAND read from offset %llx failed %d\n",
N				offset, rval);
N			*length -= left_to_read;
N			return rval;
N		}
N
N		left_to_read -= read_length;
N		offset       += read_length;
N		p_buffer     += read_length;
N	}
N
N	return 0;
N}
