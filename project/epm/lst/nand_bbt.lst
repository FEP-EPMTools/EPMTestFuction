Nstatic u32 bbt_get_ver_offs(struct mtd_info *mtd, struct nand_bbt_descr *td)
N{
N	u32 ver_offs = td->veroffs;
N
N	if (!(td->options & NAND_BBT_NO_OOB))
X	if (!(td->options & 0x00400000))
N		ver_offs += mtd->writesize;
N	return ver_offs;
