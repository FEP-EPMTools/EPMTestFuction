N	len += (len >> this->page_shift) * mtd->oobsize;
N	buf = yaffs_malloc(len);
N	if (!buf) {
N		sysprintf("nand_update_bbt: Out of memory\n");
N		return -ENOMEM;
X		return -12;
N	}
