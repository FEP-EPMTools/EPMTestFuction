N{
N	struct mtd_oob_ops ops;
N
N	ops.mode = MTD_OOB_PLACE;
N	ops.ooboffs = 0;
N	ops.ooblen = mtd->oobsize;
N	ops.datbuf = buf;
N	ops.oobbuf = oob;
N	ops.len = len;
