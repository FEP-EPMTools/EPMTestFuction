N/**
N  * @brief This function configure and start a smartcard timer of specified smartcard module.
N  * @param[in] sc Smartcard module number
N  * @param[in] u32TimerNum Timer(s) to start. Valid values are 0, 1, 2.
N  * @param[in] u32Mode Timer operating mode, valid values are:
N  *             - \ref SC_TMR_MODE_0
N  *             - \ref SC_TMR_MODE_1
N  *             - \ref SC_TMR_MODE_2
N  *             - \ref SC_TMR_MODE_3
N  *             - \ref SC_TMR_MODE_4
N  *             - \ref SC_TMR_MODE_5
N  *             - \ref SC_TMR_MODE_6
N  *             - \ref SC_TMR_MODE_7
N  *             - \ref SC_TMR_MODE_8
N  *             - \ref SC_TMR_MODE_F
N  * @param[in] u32ETUCount Timer timeout duration, ETU based. For timer 0, valid  range are between 1~0x1000000ETUs.
N  *                        For timer 1 and timer 2, valid range are between 1 ~ 0x100 ETUs.
N  * @return None
N  * @details Enable Timer starting, counter will count when condition match.
N  * @note This function start the timer within smartcard module, \b not timer module.
N  * @note Depend on the timer operating mode, timer may not start counting immediately.
N  */
Nvoid SC_StartTimer(UINT sc, UINT u32TimerNum, UINT u32Mode, UINT u32ETUCount)
N{
N    uint32_t reg = u32Mode | (0xFFFFFF & (u32ETUCount - 1));
N
N    if(sc == 0) {
N        if(u32TimerNum == 0) {
N            outpw(REG_SC0_TMRCTL0, reg);
X            *((volatile unsigned int *)((0xB8005000+0x28))) = reg;
N            outpw(REG_SC0_ALTCTL, inpw(REG_SC0_ALTCTL) | SC_ALTCTL_CNTEN0_Msk);
X            *((volatile unsigned int *)((0xB8005000+0x08))) = (*((volatile unsigned int *)((0xB8005000+0x08)))) | 0x00000020;
N        } else if(u32TimerNum == 1) {
N            outpw(REG_SC0_TMRCTL1, reg);
X            *((volatile unsigned int *)((0xB8005000+0x2C))) = reg;
N            outpw(REG_SC0_ALTCTL, inpw(REG_SC0_ALTCTL) | SC_ALTCTL_CNTEN1_Msk);
X            *((volatile unsigned int *)((0xB8005000+0x08))) = (*((volatile unsigned int *)((0xB8005000+0x08)))) | 0x00000040;
N        } else {   // timer 2
N            outpw(REG_SC0_TMRCTL2, reg);
X            *((volatile unsigned int *)((0xB8005000+0x30))) = reg;
N            outpw(REG_SC0_ALTCTL, inpw(REG_SC0_ALTCTL) | SC_ALTCTL_CNTEN2_Msk);
X            *((volatile unsigned int *)((0xB8005000+0x08))) = (*((volatile unsigned int *)((0xB8005000+0x08)))) | 0x00000080;
N        }
N    } else {
N        if(u32TimerNum == 0) {
N            outpw(REG_SC1_TMRCTL0, reg);
X            *((volatile unsigned int *)((0xB8005400+0x28))) = reg;
N            outpw(REG_SC1_ALTCTL, inpw(REG_SC1_ALTCTL) | SC_ALTCTL_CNTEN0_Msk);
X            *((volatile unsigned int *)((0xB8005400+0x08))) = (*((volatile unsigned int *)((0xB8005400+0x08)))) | 0x00000020;
N        } else if(u32TimerNum == 1) {
N            outpw(REG_SC1_TMRCTL1, reg);
X            *((volatile unsigned int *)((0xB8005400+0x2C))) = reg;
N            outpw(REG_SC1_ALTCTL, inpw(REG_SC1_ALTCTL) | SC_ALTCTL_CNTEN1_Msk);
X            *((volatile unsigned int *)((0xB8005400+0x08))) = (*((volatile unsigned int *)((0xB8005400+0x08)))) | 0x00000040;
N        } else {   // timer 2
N            outpw(REG_SC1_TMRCTL2, reg);
X            *((volatile unsigned int *)((0xB8005400+0x30))) = reg;
N            outpw(REG_SC1_ALTCTL, inpw(REG_SC1_ALTCTL) | SC_ALTCTL_CNTEN2_Msk);
X            *((volatile unsigned int *)((0xB8005400+0x08))) = (*((volatile unsigned int *)((0xB8005400+0x08)))) | 0x00000080;
N        }
N
N    }
N}
