N		if (chip->ecc.prepad) {
N			chip->write_buf(mtd, oob, chip->ecc.prepad);
N			oob += chip->ecc.prepad;
N		}
N
N		chip->ecc.calculate(mtd, p, oob);
N		chip->write_buf(mtd, oob, eccbytes);
