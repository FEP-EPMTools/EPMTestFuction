Sstatic __inline void __change_bit(int nr, volatile void *addr)
S{
S	unsigned long mask = BIT_MASK(nr);
S	unsigned long *p = ((unsigned long *)addr) + BIT_WORD(nr);
S
S	*p ^= mask;
S}
