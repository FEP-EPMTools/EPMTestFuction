Sstatic inline int __test_and_clear_bit(int nr, volatile void *addr)
S{
S	unsigned long mask = BIT_MASK(nr);
S	unsigned long *p = ((unsigned long *)addr) + BIT_WORD(nr);
S	unsigned long old = *p;
S
S	*p = old & ~mask;
S	return (old & mask) != 0;
S}
