N/* Put a character to the file                                           */
N/*-----------------------------------------------------------------------*/
N
Ntypedef struct {
N	FIL* fp;
N	int idx, nchr;
N	BYTE buf[64];
N} putbuff;
N
N
Nstatic
Nvoid putc_bfd (
N	putbuff* pb,
N	TCHAR c
N)
N{
N	UINT bw;
N	int i;
N
N
N	if (_USE_STRFUNC == 2 && c == '\n')	 /* LF -> CRLF conversion */
X	if (1 == 2 && c == '\n')	  
N		putc_bfd(pb, '\r');
N
N	i = pb->idx;	/* Buffer write index (-1:error) */
N	if (i < 0) return;
N
N#if _USE_LFN && _LFN_UNICODE
X#if 2 && 0
S#if _STRF_ENCODE == 3			/* Write a character in UTF-8 */
S	if (c < 0x80) {				/* 7-bit */
S		pb->buf[i++] = (BYTE)c;
S	} else {
S		if (c < 0x800) {		/* 11-bit */
S			pb->buf[i++] = (BYTE)(0xC0 | c >> 6);
S		} else {				/* 16-bit */
S			pb->buf[i++] = (BYTE)(0xE0 | c >> 12);
S			pb->buf[i++] = (BYTE)(0x80 | (c >> 6 & 0x3F));
S		}
S		pb->buf[i++] = (BYTE)(0x80 | (c & 0x3F));
S	}
S#elif _STRF_ENCODE == 2			/* Write a character in UTF-16BE */
S	pb->buf[i++] = (BYTE)(c >> 8);
S	pb->buf[i++] = (BYTE)c;
S#elif _STRF_ENCODE == 1			/* Write a character in UTF-16LE */
S	pb->buf[i++] = (BYTE)c;
S	pb->buf[i++] = (BYTE)(c >> 8);
S#else							/* Write a character in ANSI/OEM */
S	c = ff_convert(c, 0);	/* Unicode -> OEM */
S	if (!c) c = '?';
S	if (c >= 0x100)
S		pb->buf[i++] = (BYTE)(c >> 8);
S	pb->buf[i++] = (BYTE)c;
S#endif
N#else							/* Write a character without conversion */
N	pb->buf[i++] = (BYTE)c;
N#endif
N
