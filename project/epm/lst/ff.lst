Nstatic
Nint cmp_lfn (			/* 1:Matched, 0:Not matched */
N	WCHAR* lfnbuf,		/* Pointer to the LFN to be compared */
N	BYTE* dir			/* Pointer to the directory entry containing a part of LFN */
N)
N{
